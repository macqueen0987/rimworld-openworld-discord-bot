{
    // your discord id in int form
    // Don't need to change if you are not going to run a bot
    "me" : 1111,

    // your bot secret token
    // Don't need to change if you are not going to run a bot
    "token" : "your_some_ver_secret_token",

// ==============================================================SOME LINE FOR ORGANIZATION===================================================
    // -----------------------------bot thingys-------------------
    // prefix for the bot.
    "prefix": "!",

    // prefix commands that will be sent to server
    "server_prefix": "#",

    // time you will wait for the log, if too short you may not get the logs from inputs
    "wait_for_log" : 1,

    // max line for bot output because discord has max amount of letters you can send
    "max_line":45,

    // max line for bot output becaus discord has max amount of letters you can send
    "mention_author":false,

    // -----------------------------common thingys-------------------
    // server directory where steamcmd.sh file is located. MUST BE ABSOLUTE PATH
    "steam_dir" : "C:/Users/user/Desktop/steamcmd/",

    // auto update mod interval in hours
    "auto_update_mods":24,

    // if you are going to use the auto update Thing
    // For those whe are not going to run discord bot, making this true will update the mods at the start, and keep auto updating
    "auto_update":true,

    // if you are going to download the dlc and cores when you update the mods
    "use_dlc":true,

    // a link to dlc and cores. please do not change
    "dlc_link": "https://github.com/TastyLollipop/OpenWorld/raw/main/Core%20%26%20DLCs.zip",

// ====================================================SPACE FOR MODS=============================================

    // neccessary mods steam collection id, set null if it doesn't exist
    // id can be found in mods collection page url, after the filedetails/?id=
    "mods":null,

    // whitelist mods steam collection id, set null if it doesn't exist
    // id can be found in mods collection page url, after the filedetails/?id=
    "whitelist_mods":null
}